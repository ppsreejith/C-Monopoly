define(function(){var jsfxlib={};(function(){this.createWaves=function(a){var b={},d;for(d in a)b[d]=this.createWave(a[d]);return b};this.createWave=function(a){a=this.arrayToParams(a);a=jsfx.generate(a);return audio.make(a)};this.paramsToArray=function(a){for(var b=[],d=jsfx.Parameters.length,c=0;c<d;c++)b.push(a[jsfx.Parameters[c].id]);return b};this.arrayToParams=function(a){for(var b={},d=jsfx.Parameters.length,c=0;c<d;c++)b[jsfx.Parameters[c].id]=a[c];return b}}).apply(jsfxlib);return jsfxlib;});
